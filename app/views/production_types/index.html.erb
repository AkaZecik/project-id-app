<p id="notice"><%= notice %></p>

<h1>Production Types</h1>

<table>
  <thead>
    <tr>
      <th>Production type</th>
      <th>Name</th>
      <th>Super type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @production_types.each do |production_type| %>
      <tr>
        <td><%= production_type.production_type_id %></td>
        <td><%= production_type.name %></td>
        <td><%= production_type.super_type %></td>
        <td><%= link_to 'Show', production_type %></td>
        <td><%= link_to 'Edit', edit_production_type_path(production_type) %></td>
        <td><%= link_to 'Destroy', production_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Production Type', new_production_type_path %>
